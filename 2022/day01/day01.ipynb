   "source": [
    "%time\n",
    "with open(\"input.txt\") as f:\n",
    "    data = [[int(e) for e in elf.splitlines()] for elf in f.read().split(\"\\n\\n\")]\n",
    "\n",
    "li = []\n",
    "for meal in data:\n",
    "    li.append(sum(meal))\n",
    "    \n",
    "print(sorted(li)[-1])\n",
    "print(sum(sorted(li)[-3:]))"
   ]
  }
